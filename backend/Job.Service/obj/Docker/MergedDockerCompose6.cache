services:
  JobDB:
    container_name: job-db
    image: mcr.microsoft.com/mssql/server:2019-latest
    networks:
      default: null
  api:
    build:
      context: C:\Users\fitor\ProjektiLab2\NetWorked\backend\Job.Service
      dockerfile: API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: API
      target: base
    container_name: api
